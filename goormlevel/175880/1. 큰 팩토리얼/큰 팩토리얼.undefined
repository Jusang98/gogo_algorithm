// Run by Node.js
const readline = require('readline');

(async () => {
	let rl = readline.createInterface({ input: process.stdin });
	
	for await (const line of rl) {
		let cleanLine = line.trim();
		if(!cleanLine)continue;
		let num = Number(cleanLine);
		let result = 1;
		let MOD = 1000000007;
		for(let i = 1; i<=num;i++){
			result=(result*i)%MOD
		}
		console.log(result)
		rl.close();
	}
	
	process.exit();
})();
